  (so12:SalesOrder {sid:"SO1603", superType:"T"})
  (q1)<-[so11q1:basedOn]-(so11)-[so11acme:receivedFrom]->(acme)
  (q1)<-[so12q1:basedOn]-(so12)-[so12acme:receivedFrom]->(acme)
  (so11)-[so11pear:contains {qty:8}]->(pear)
  (so12)-[so12apple:contains {qty:8}]->(apple)

  (q2:Quotation {sid:"QN1602", status:"closed", superType:"T"})
