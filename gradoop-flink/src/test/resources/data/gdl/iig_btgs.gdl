(acme:Customer {sid:"Cus-ACME", superType:"M"})
(apple:Product {sid:"Prd-Apple", superType:"M"})
(pear:Product {sid:"Prd-Pear", superType:"M"})
(orange:Product {sid:"Prd-Orange", superType:"M"})
(grapes:Product {sid:"Prd-Grapes", superType:"M"})
(alice:Employee {sid:"Emp-Alice", superType:"M"})
(bob:Employee {sid:"Emp-Bob", superType:"M"})
(carol:Employee {sid:"Emp-Carol", superType:"M"})

iig:IntegratedInstanceGraph[

  (q1:Quotation {sid:"QN1601", status:"closed", superType:"T"})
  (q1)-[q1acme:sentTo]->(acme)
  (q1)-[q1apple:contains {qty:5}]->(apple)
  (q1)-[q1pear:contains {qty:10}]->(pear)
  (pc1:PhoneCall {sid:"PC123", superType:"T"})
