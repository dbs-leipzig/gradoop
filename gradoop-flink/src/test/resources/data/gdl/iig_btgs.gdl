  (q4:Quotation {sid:"QN1604", status:"open", superType:"T"})
  (q4)-[q4acme:sentTo]->(acme)
  (q4)-[q4apple:contains {qty:1}]->(apple)
  (q4)-[q4pear:contains {qty:10}]->(pear)
  (pc4:PhoneCall {sid:"PC133", superType:"T"})
  (q4)<-[pc4q4:refersTo]-(pc4)-[pc4carol:doneBy]->(carol)
]
