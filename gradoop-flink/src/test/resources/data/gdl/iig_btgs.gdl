  (q2)<-[so2q2:basedOn]-(so2)-[so2acme:receivedFrom]->(acme)
  (so2)-[so2grapes:contains {qty:3}]->(grapes)

  (q3:Quotation {sid:"QN1603", status:"closed", superType:"T"})
  (q3)-[q3acme:sentTo]->(acme)
  (q3)-[q3apple:contains {qty:10}]->(apple)
  (q3)-[q3pear:contains {qty:10}]->(pear)
