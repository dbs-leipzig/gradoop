|Network{social=false:Boolean}|
 (User{name=Alice:String})
  -[hates{}]-(User{name=Bob:String})
  -[hates{}]-(User{name=Carol:String})
 (User{name=Bob:String})
  -[hates{}]-(User{name=Alice:String})
  -[knows{since=2015:Integer}]-(User{name=Carol:String})
 (User{name=Carol:String})
  -[hates{}]-(User{name=Alice:String})
  -[knows{since=2015:Integer}]-(User{name=Bob:String})
  -[loves{}]&[loves{}]-(User{name=Carol:String})
|Network{social=true:Boolean}|
 (User{name=Alice:String})
  -[knows{since=2013:Integer}]&[knows{since=2014:Integer}]-(User{name=Bob:String})
 (User{name=Bob:String})
  -[knows{since=2013:Integer}]&[knows{since=2014:Integer}]-(User{name=Alice:String})
  -[knows{since=2015:Integer}]-(User{name=Carol:String})
 (User{name=Carol:String})
  -[knows{since=2015:Integer}]-(User{name=Bob:String})