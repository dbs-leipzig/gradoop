<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/DECORATION/1.6.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.6.0 http://maven.apache.org/xsd/decoration-1.6.0.xsd"
         name="Gradoop">
    <custom>
        <fluidoSkin>
            <gitHub>
                <projectId>dbs-leipzig/gradoop</projectId>
                <ribbonOrientation>right</ribbonOrientation>
                <ribbonColor>orange</ribbonColor>
            </gitHub>
        </fluidoSkin>
    </custom>
    <bannerLeft>
        <name>Gradoop</name>
        <src>https://dbs.uni-leipzig.de/file/gradoop.png</src>
        <alt>Gradoop</alt>
    </bannerLeft>
    <body>
        <menu ref="parent" inherit="top"/>
        <menu ref="reports" inherit="top"/>
        <menu ref="modules" inherit="top"/>
        <menu name="Wiki" inherit="bottom">
            <item name="Gradoop Wiki" href="https://github.com/dbs-leipzig/gradoop/wiki"/>
        </menu>
    </body>
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.7</version>
    </skin>
</project>
